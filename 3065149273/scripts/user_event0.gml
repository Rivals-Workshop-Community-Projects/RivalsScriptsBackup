    case CH_ABSA: case CH_RANNO: case CH_OLYMPIA: case CH_HODAN: case CH_POMME:
    case "2136440419": case "1913517643": case "1943759600": case "1866016173": case "2239235956": //WS hodan / WS pomme / WS olympia / guadua
        chao_type = 0;
        break;
    case CH_ORCANE: case CH_KRAGG: case CH_FORSBURN: case CH_MAYPUL: case CH_ETALUS: case CH_CLAIREN: case CH_ORI: case CH_SHOVEL_KNIGHT:
